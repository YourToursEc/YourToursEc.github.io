{
  "version": 2,
  "builds": [
    {
      "src": "api/pago.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/respaldo.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/pago",
      "dest": "/api/pago.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/respaldo.html"
    }
  ]
}
